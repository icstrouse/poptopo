<div id="<%= dom_id tag %>">
  <p class="my-5">
    <strong class="font-medium mb-1">Name:</strong>
    <%= tag.name %>
  </p>

  <p class="my-5">
    <strong class="font-medium mb-1">Lat:</strong>
    <%= tag.lat %>
  </p>

  <p class="my-5">
    <strong class="font-medium mb-1">Lng:</strong>
    <%= tag.lng %>
  </p>

  <p class="my-5">
    <strong class="font-medium mb-1">User:</strong>
    <%= tag.user_id %>
  </p>
</div>
