<div id="<%= dom_id track %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= track.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Data:</strong>
    <%= track.data %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= track.user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Tag:</strong>
    <%= track.tag_id %>
  </p>

</div>
